#!/usr/bin/env bash

_here=$(cd "${BASH_SOURCE[0]%/*}" && pwd)
_command="$1"

. "$_here/../lib/source.sh"

OMNIA_MSG_LIMIT=20

#TODO: maybe use `exec`, but exported unctions won't work probably
case "$_command" in
	publish) ssb-server publish . <<< "$2";;
	pull) pullLatestFeedMsgOfType "$2" "$3";;
esac
